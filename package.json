{
  "name": "storybook-addon-mock-date",
  "version": "0.5.0",
  "description": "Storybook addon to mocking date",
  "keywords": [
    "storybook-addons",
    "mock-date"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/k35o/storybook-addon-mock-date.git"
  },
  "type": "module",
  "license": "MIT",
  "author": "k35o <kosakanoki@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build:watch 'storybook --quiet'",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^14.0.0",
    "@storybook/addon-docs": "next",
    "@storybook/addon-links": "next",
    "@storybook/blocks": "next",
    "@storybook/components": "next",
    "@storybook/core-events": "next",
    "@storybook/icons": "^1.4.0",
    "@storybook/manager": "next",
    "@storybook/preview": "next",
    "@storybook/react": "next",
    "@storybook/react-vite": "next",
    "@storybook/test": "next",
    "@storybook/theming": "next",
    "@storybook/types": "next",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/sinonjs__fake-timers": "^8.1.5",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.1",
    "boxen": "^8.0.1",
    "dedent": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "next",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vite": "^6.3.5",
    "zx": "^8.5.4"
  },
  "peerDependencies": {
    "storybook": "^9.0.0"
  },
  "dependencies": {
    "util": "^0.12.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Mocking Date",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
